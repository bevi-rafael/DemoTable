<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="br.com.demotable.DemoTable.controller.View1" displayBlock="true">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
				    <Table noDataText="Drop column list items here and columns in the area above" id="__table0" items="{/SalesOrderSet}">
				        <items>
				            <ColumnListItem type="Active" id="__item0">
				                <cells>
				                    <Text text="{SalesOrderID}" />
				                    <Text text="{CustomerName}" />
				                 </cells>
				            </ColumnListItem>
				        </items>
				        <columns>
				            <Column id="__column0">
				                <header>
				                    <Label text="ProductID" id="__label0"/>
				                </header>
				            </Column>
				            <Column id="__column1">
				                <header>
				                    <Label text="Name" id="__label1"/>
				                </header>
				            </Column>
				         </columns>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>